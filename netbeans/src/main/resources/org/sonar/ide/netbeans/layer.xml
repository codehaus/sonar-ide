<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">

<filesystem>
  <folder name="Editors">
    <folder name="AnnotationTypes">
      <file name="org-sonar-ide-netbeans-sonar-annotation.xml"
            url="nbresloc:/org/sonar/ide/netbeans/sonar-annotation.xml"/>
    </folder>
  </folder>

  <folder name="TaskList">
    <folder name="Groups">
      <file name="org-sonar-ide-netbeans-tasklist-SonarTaskGroup.instance">
        <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>

        <attr name="localizingBundle" stringvalue="org.sonar.ide.netbeans.tasklist.Bundle"/>
        <attr name="groupName" stringvalue="org-sonar-ide-netbeans-Task"/>
        <attr name="diplayNameKey" stringvalue="SonarTaskGroup.label"/>
        <attr name="descriptionKey" stringvalue="SonarTaskGroup.hint"/>
        <attr name="iconKey" stringvalue="SonarTaskGroup.icon"/>
        <attr name="position" intvalue="250"/>
      </file>
    </folder>

    <folder name="Scanners">
      <file name="org-sonar-ide-netbeans-tasklist-SonarTaskScanner.instance">
        <attr name="instanceClass" stringvalue="org.sonar.ide.netbeans.tasklist.SonarTaskScanner"/>
      </file>
    </folder>
  </folder>
</filesystem>
